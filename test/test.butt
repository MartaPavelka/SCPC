#!/bin/bash

declare -i i=0
declare status=
declare -a outlines=()
declare -a errlines=()
declare -r scpc="python scpc.py"

jupyter nbconvert --to python scpc.ipynb >/dev/null 2>&1
chmod +x scpc.py

#######################################
i+=1
start "help"
  debug "$scpc --help"
  assert_equal "$status" 0
  assert_startwith "${outlines[0]}" "Usage: scpc.py [options]"
end
#######################################
